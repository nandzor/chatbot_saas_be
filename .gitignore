# =======================
# Chatbot SaaS Backend - .gitignore
# =======================
#
# Comprehensive .gitignore for Laravel 12 + FrankenPHP + Docker setup
# Covers: Laravel, PHP, Docker, IDEs, OS files, databases, logs, and more
#
# Project stack:
# - Laravel 12 with FrankenPHP
# - PostgreSQL + Redis + RabbitMQ
# - Docker containerization
# - API development with Sanctum auth
#
# =======================
# Laravel & PHP
# =======================

# Environment files
.env
.env.*
!.env.example
!env.example

# Application cache
/bootstrap/cache/*.php
!/bootstrap/cache/.gitkeep

# Storage files
/storage/*.key
/storage/pail
/storage/logs/*.log
/storage/app/*
!/storage/app/.gitkeep
!/storage/app/public/.gitkeep
/storage/framework/cache/*
!/storage/framework/cache/.gitkeep
/storage/framework/sessions/*
!/storage/framework/sessions/.gitkeep
/storage/framework/views/*
!/storage/framework/views/.gitkeep

# Public storage symlink
/public/storage

# Dependencies
/vendor/
/node_modules/
auth.json

# Laravel-specific
.phpunit.result.cache
/.phpunit.cache
/phpunit.xml.bak

# Build artifacts
