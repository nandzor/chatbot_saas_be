{
  "rabbit_version": "3.12.0",
  "rabbitmq_version": "3.12.0",
  "users": [
    {
      "name": "admin",
      "password_hash": "password",
      "hashing_algorithm": "rabbit_password_hashing_sha256",
      "tags": "administrator"
    }
  ],
  "vhosts": [
    {
      "name": "/"
    }
  ],
  "permissions": [
    {
      "user": "admin",
      "vhost": "/",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ],
  "topic_permissions": [],
  "parameters": [],
  "global_parameters": [
    {
      "name": "cluster_name",
      "value": "chatbot_saas_cluster"
    }
  ],
  "policies": [
    {
      "vhost": "/",
      "name": "ha-all",
      "pattern": ".*",
      "apply-to": "all",
      "definition": {
        "ha-mode": "all"
      },
      "priority": 0
    }
  ],
  "queues": [
    {
      "name": "default",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 3600000,
        "x-max-length": 10000
      }
    },
    {
      "name": "high_priority",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 1800000,
        "x-max-length": 5000,
        "x-max-priority": 10
      }
    },
    {
      "name": "notifications",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 7200000,
        "x-max-length": 20000
      }
    }
  ],
  "exchanges": [
    {
      "name": "laravel_direct",
      "vhost": "/",
      "type": "direct",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "laravel_topic",
      "vhost": "/",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "bindings": [
    {
      "source": "laravel_direct",
      "vhost": "/",
      "destination": "default",
      "destination_type": "queue",
      "routing_key": "default",
      "arguments": {}
    },
    {
      "source": "laravel_direct",
      "vhost": "/",
      "destination": "high_priority",
      "destination_type": "queue",
      "routing_key": "high_priority",
      "arguments": {}
    },
    {
      "source": "laravel_topic",
      "vhost": "/",
      "destination": "notifications",
      "destination_type": "queue",
      "routing_key": "notifications.*",
      "arguments": {}
    }
  ]
}
