# Application Configuration
APP_NAME="Chatbot SaaS API"
APP_ENV=production
APP_KEY=
APP_DEBUG=false
APP_TIMEZONE=UTC
APP_URL=http://localhost:9000
APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US
APP_MAINTENANCE_DRIVER=file
APP_MAINTENANCE_STORE=database

# Database Configuration
DB_CONNECTION=pgsql
DB_HOST=postgres
DB_PORT=5432
DB_DATABASE=chatbot_saas
DB_USERNAME=postgres
DB_PASSWORD=kambin

# Redis Configuration
REDIS_CLIENT=predis
REDIS_HOST=redis
REDIS_PASSWORD=null
REDIS_PORT=6379
REDIS_DB=0

# Cache Configuration
CACHE_STORE=redis
CACHE_PREFIX=chatbot_saas_cache

# Session Configuration
SESSION_DRIVER=redis
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

# Queue Configuration
QUEUE_CONNECTION=rabbitmq

# RabbitMQ Configuration
RABBITMQ_HOST=rabbitmq
RABBITMQ_PORT=5672
RABBITMQ_USER=admin
RABBITMQ_PASSWORD=kambin
RABBITMQ_VHOST=/
RABBITMQ_EXCHANGE=laravel_direct
RABBITMQ_EXCHANGE_TYPE=direct
RABBITMQ_EXCHANGE_PASSIVE=false
RABBITMQ_EXCHANGE_DURABLE=true
RABBITMQ_EXCHANGE_AUTO_DELETE=false
RABBITMQ_EXCHANGE_ARGUMENTS=

# Broadcasting Configuration
BROADCAST_CONNECTION=redis
BROADCAST_DRIVER=reverb

# Reverb Configuration
REVERB_APP_ID=chatbot_saas
REVERB_APP_KEY=local-key
REVERB_APP_SECRET=local-secret
REVERB_HOST="localhost"
REVERB_PORT=9000
REVERB_SCHEME=http

# Mail Configuration
MAIL_MAILER=log
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

# AWS Configuration (for S3 storage)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

# Logging Configuration
LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

# Security Configuration
BCRYPT_ROUNDS=12

# API Configuration
API_RATE_LIMIT=60
API_RATE_LIMIT_WINDOW=1

# JWT Configuration (if using JWT)
JWT_SECRET=
JWT_TTL=60
JWT_REFRESH_TTL=20160

# File Storage
FILESYSTEM_DISK=local

# Health Check Configuration
HEALTH_ENABLED=true

# Backup Configuration
BACKUP_DISK=s3

# Performance Configuration
OCTANE_SERVER=frankenphp

# Development Configuration
TELESCOPE_ENABLED=false
DEBUGBAR_ENABLED=false
